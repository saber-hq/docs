{
  "name": "my-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "lint": "eslint . --cache",
    "lint:ci": "eslint . --max-warnings=0",
    "swizzle": "docusaurus swizzle",
